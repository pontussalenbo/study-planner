{
	"name": "studyplan",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"prettier:check": "prettier --check src",
		"prettier:fix": "prettier --write src",
		"lint": "eslint src",
		"prepare": "cd ../ && husky install ./StudyPlannerUI/.husky"
	},
	"dependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"recharts": "^2.3.2",
		"styled-components": "^5.3.6"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.4.4",
		"@commitlint/config-conventional": "^17.4.4",
		"@faker-js/faker": "^7.6.0",
		"@nabla/vite-plugin-eslint": "1.5.0",
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.9",
		"@types/styled-components": "^5.1.26",
		"@typescript-eslint/eslint-plugin": "5.43.0",
		"@typescript-eslint/parser": "5.43.0",
		"@vitejs/plugin-react": "^3.0.0",
		"@vitest/coverage-istanbul": "0.25.2",
		"autoprefixer": "^10.4.13",
		"commitizen": "^4.3.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.32.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-react": "^7.32.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-prefer-function-component": "^3.1.0",
		"eslint-plugin-testing-library": "^5.9.1",
		"eslint-plugin-unicorn": "^45.0.2",
		"husky": "^8.0.0",
		"lint-staged": "^13.1.2",
		"postcss": "^8.4.21",
		"prettier": "^2.8.3",
		"stylelint": "14.15.0",
		"stylelint-config-prettier": "9.0.4",
		"stylelint-config-standard": "29.0.0",
		"tailwindcss": "^3.2.4",
		"typescript": "^4.9.3",
		"vite": "^4.0.0",
		"vite-plugin-pwa": "0.13.1",
		"vite-tsconfig-paths": "3.5.2",
		"vitest": "^0.29.1",
		"workbox-build": "6.5.4",
		"workbox-window": "6.5.4"
	},
	"browserslist": {
		"production": "Edge >= 18, Firefox >= 60, Chrome >= 61, Safari >= 11, Opera >= 48",
		"development": [
			"last 1 chrome version",
			"last 1 firefox version"
		]
	},
	"config": {
		"commitizen": {
			"path": "./StudyPlannerUI/node_modules/cz-conventional-changelog"
		}
	},
	"lint-staged": {
		"*.{css,less,scss,html,json,jsx,js}": [
			"prettier --write"
		],
		"*.{ts,tsx}": [
			"prettier --write",
			"eslint --fix"
		]
	},
	"eslintIgnore": [
		"vite-env.d.ts"
	]
}
