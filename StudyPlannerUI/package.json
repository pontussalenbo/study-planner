{
  "name": "studyplan",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "set DEBUG=app & vite",
    "build": "rm -rf build && tsc && vite build",
    "test": "vitest",
    "preview": "vite preview",
    "prettier:check": "prettier --check src",
    "prettier:fix": "prettier --write src",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "prepare": "cd ../ && husky install ./StudyPlannerUI/.husky"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.5.0",
    "styled-components": "^5.3.10",
    "vite-plugin-svgr": "^3.2.0"
  },
  "devDependencies": {
    "vite-plugin-svgr": "^3.2.0",
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@faker-js/faker": "^7.6.0",
    "@nabla/vite-plugin-eslint": "1.5.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/debug": "^4.1.7",
    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.0.11",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "5.59.1",
    "@typescript-eslint/parser": "5.59.1",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-istanbul": "0.30.1",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-recharts": "^2.0.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.39.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-no-inline-styles": "^1.0.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-prefer-function-component": "^3.1.0",
    "eslint-plugin-testing-library": "^5.10.3",
    "eslint-plugin-unicorn": "^46.0.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.8",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "stylelint": "15.6.0",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-standard": "33.0.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.1",
    "vite-plugin-importus": "^1.0.4",
    "vite-plugin-pwa": "0.14.7",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "^0.30.1",
    "workbox-build": "6.5.4",
    "workbox-window": "6.5.4"
  },
  "browserslist": {
    "production": "Edge >= 18, Firefox >= 60, Chrome >= 61, Safari >= 11, Opera >= 48",
    "development": [
      "last 1 chrome version",
      "last 1 firefox version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./StudyPlannerUI/node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{css,less,scss,html,json,jsx,js}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "eslintIgnore": [
    "vite-env.d.ts"
  ]
}
